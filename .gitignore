# brew install interactive-rebase-tool git-lfs

# Ignore bundler config.
/.bundle

# Ignore developer archives
*.zip
*.tar
*.tar.gz
*.tar.bz2

# Ignore the default SQLite database
*.sql

# Ignore all logfiles and tempfiles
/db/*.sqlite3
/db/*.sqlite3-journal
/log/*.log
log/*.json
/tmp

# Ignore app stuff
.env*
.vagrant
application.yml
config/ssl/*
public/assets/*
spec/examples.txt
vendor/*
